<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:state_selected="false">
        <shape>
            <corners android:radius="30dp" />
            <solid android:color="@color/orange_lite" />
        </shape>
    </item>
    <item android:state_checked="true" android:state_selected="true">
        <shape>
            <corners android:radius="30dp" />
            <stroke android:width="1dp" android:color="@color/textColor9" />
        </shape>
    </item>
</selector>